{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "moduleResolution": "node"
  },
  "include": ["**/*"],
  "exclude": ["node_modules", ".vscode", "functions", "dist"]
}
